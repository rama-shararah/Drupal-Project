<div id="fh5co-page">

   <header id="fh5co-header" role="banner">
      <div class="container">
         <div class="header-inner">
            {{ page.site_branding }}
               {{ page.primary_menu }} 
         </div>
      </div>
   </header>
   
   <!-- Tabs -->
   {{ page.tabs }}
   <!-- Content -->
   {{ page.content }}
   
   <footer id="fh5co-footer" role="contentinfo">
      <div class="container">
         <div class="col-md-3 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
            <h3>{% if about_us_title|render is not empty %}{{ about_us_title }}{% endif %}</h3>
            {% if about_desc %}
               <p>{{ about_desc|raw }}</p>
            {% endif %}
            {% if about_us_url|render is not empty %}
               <p><a href="{{ about_us_url }}" class="btn btn-primary btn-outline with-arrow btn-sm">Read More <i class="icon-arrow-right"></i></a></p>
            {% endif %}
         </div>
         <div class="col-md-3 col-md-push-1 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
            {{ page.latest_blog }}
         </div>
         <div class="col-md-3 col-md-push-1 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
            {{ page.footer_menu }}
         </div>
         <div class="col-md-2 col-md-push-1 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
            <h3>{% if social_title|render is not empty %}{{ social_title }}{% endif %}</h3>
            <ul class="fh5co-social">
               {% if twitter_url|render is not empty %}
                  <li><a href="{{ twitter_url }}"><i class="icon-twitter"></i></a></li>
               {% endif %}
               {% if facebook_url|render is not empty %}
                  <li><a href="{{ facebook_url }}"><i class="icon-facebook"></i></a></li>
               {% endif %}
               {% if google_plus_url|render is not empty %}
                  <li><a href="{{ google_plus_url }}"><i class="icon-google-plus"></i></a></li>
               {% endif %}
               {% if instagram_url|render is not empty %}
                 <li><a href="{{ instagram_url }}"><i class="icon-instagram"></i></a></li>
               {% endif %}     
            </ul>
         </div>
         <div class="col-md-12 fh5co-copyright text-center">
            <p><span> &copy; 2018 <i>YG Bold</i> All Rights Reserved. Designed with <i class="icon-heart"></i> by <a href="http://freehtml5.co" target="_blank">FREEHTML5.co</a><br> Theme By<a href="https://www.drupaldevelopersstudio.com" target="_blank"> Drupal Developers Studio</a>, A Division of <a href="http://www.youngglobes.com" target="_blank">Young Globes</a></span></p>  
         </div>
      </div>
   </footer>
</div>
      
   