<!-- Pricing -->
<div class="fh5co-pricing">
	<div class="container">	
		<div class="row">
			<div class="col-md-6 col-md-offset-3 text-center fh5co-heading animate-box">
			<h2>{{ content.field_title.0 }}</h2>
			{{ content.field_body.0 }}
		</div>
			<div class="pricing">
				{{ content.field_pricing_items }}
			</div>
		</div>
		<div class="row">
			<div class="col-md-6 col-md-offset-3 animate-box">
				<p>{{ content.field_phone_number.0 }} 
				{% if content.field_url.0|render is not empty %}
					<a href="{{ content.field_url[0]['#url']|render }}">
					{{ content.field_url[0]['#title'] }}</a>
				{% endif %}	
				</p>
			</div>
		</div>
	</div>
</div>
<!-- Pricing End-->
