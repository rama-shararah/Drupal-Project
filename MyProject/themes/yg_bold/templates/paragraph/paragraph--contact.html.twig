<!-- Contact Info -->
	<div class="fh5co-contact animate-box">
		<div class="container">
			<div class="row">
				<div class="col-md-3">
					<h3>{{ content.field_title.0 }}</h3>
					<ul class="contact-info">
						<li><i class="icon-map"></i>{{ content.field_body.0 }}</li>
						<li><i class="icon-phone"></i>{{ content.field_phone_number.0 }}</li>
						{% if content.field_email.0|render is not empty %}
							<li><i class="icon-envelope"></i>
							   <a href="mailto:{{ content.field_email.0 }}">{{ content.field_email.0 }}</a>
							</li>
						{% endif %}
						{% if content.field_url.0|render is not empty %}
							<li><i class="icon-globe"></i>
								<a href="{{ content.field_url[0]['#url']|render }}">{{ content.field_url[0]['#title'] }}</a>
							</li>
				       	{% endif %} 
					</ul>
				</div>
				<div class="col-md-8 col-md-push-1 col-sm-12 col-sm-push-0 col-xs-12 col-xs-push-0">
					<div class="row">
						{{ content.field_contact_form.0 }}
					</div>
				</div>
			</div>
		</div>	
	</div>

	<div id="map" class="animate-box" data-animate-effect="fadeIn">
		{{ content.field_map.0 }}
	</div>

<!-- Contact Info End-->

